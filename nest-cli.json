{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth-service",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "clients-service": {
      "type": "application",
      "root": "apps/clients-service",
      "entryFile": "main",
      "sourceRoot": "apps/clients-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/clients-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "contracts-service": {
      "type": "application",
      "root": "apps/contracts-service",
      "entryFile": "main",
      "sourceRoot": "apps/contracts-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/contracts-service/tsconfig.app.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    },
    "files-service": {
      "type": "application",
      "root": "apps/files-service",
      "entryFile": "main",
      "sourceRoot": "apps/files-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files-service/tsconfig.app.json"
      }
    },
    "orders-service": {
      "type": "application",
      "root": "apps/orders-service",
      "entryFile": "main",
      "sourceRoot": "apps/orders-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders-service/tsconfig.app.json"
      }
    },
    "products-service": {
      "type": "application",
      "root": "apps/products-service",
      "entryFile": "main",
      "sourceRoot": "apps/products-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/products-service/tsconfig.app.json"
      }
    },
    "services-service": {
      "type": "application",
      "root": "apps/services-service",
      "entryFile": "main",
      "sourceRoot": "apps/services-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/services-service/tsconfig.app.json"
      }
    },
    "suppliers-service": {
      "type": "application",
      "root": "apps/suppliers-service",
      "entryFile": "main",
      "sourceRoot": "apps/suppliers-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/suppliers-service/tsconfig.app.json"
      }
    },
    "work-order-management": {
      "type": "application",
      "root": "apps/work-order-management",
      "entryFile": "main",
      "sourceRoot": "apps/work-order-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/work-order-management/tsconfig.app.json"
      }
    }
  }
}
